<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
  	<context id="default" targetRuntime="MyBatis3">
  	
    	<jdbcConnection driverClass="org.postgresql.Driver"
			connectionURL="jdbc:postgresql://localhost:5432/team-dev-test"
			userId="training"
			password="worker">
		</jdbcConnection>
		
    	<javaModelGenerator targetPackage="com.example.demo.model" targetProject="NHS/src/main/java">
			<property name="enableSubPackages" value="true"/>
			<property name="trimStrings" value="true"/>
		</javaModelGenerator>
		
		<sqlMapGenerator targetPackage="com.example.demo.mapper" targetProject="NHS/src/main/resources">
			<property name="enableSubPackages" value="true"/>
		</sqlMapGenerator>
		
    	<javaClientGenerator targetPackage="com.example.demo.mapper" targetProject="NHS/src/main/java" type="XMLMAPPER">
			<property name="enableSubPackages" value="true"/>
		</javaClientGenerator>
		
    	<table tableName="users" domainObjectName="User">
			<columnOverride column="user_id" property="userId"/>
			<columnOverride column="email" property="email"/>
			<columnOverride column="password" property="password"/>
			<columnOverride column="created_at" javaType="java.time.LocalDate" property="createdDate"/>
		</table>
		
		<table tableName="introductions" domainObjectName="Introduction">
			<columnOverride column="introduction_id" property="introductionId" />
			<columnOverride column="user_id" property="userId" />
			<columnOverride column="name" property="name" />
			<columnOverride column="kana" property="kana" />
			<columnOverride column="gender" property="gender" />
			<columnOverride column="hobby" property="hobby" />
			<columnOverride column="word" property="word" />
			<columnOverride column="created_at" javaType="java.time.LocalDate" property="createdDate" />
			<columnOverride column="updated_at" javaType="java.time.LocalDate" property="updatedDate" />
		</table>
		
  	</context>
</generatorConfiguration>